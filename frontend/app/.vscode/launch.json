
// .vscode/launch.json
{
    "version": "0.2.0",
    "configurations": [
    {
    "name": "Python: Flask (frontend)",
    "type": "python",
    "request": "launch",
    "program": "/app/app/main.py",
    "args": [],
    "cwd": "/app/app",
    "env": {
    "FLASK_APP": "app/main.py",
    "FLASK_ENV": "development"
    },
    "stopOnEntry": false,
    "console": "integratedTerminal",
    "internalConsoleOptions": "neverOpen"
    }
    ]
    }
    
    
    
    